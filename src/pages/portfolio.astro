---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Portfolio from '../components/Portfolio.astro';
import Contact from '../components/Contact.astro';
import Footer from '../components/Footer.astro';

const pageTitle = "Portfolio | prestashop.supports.pro";
const pageDescription = "Explore our portfolio of PrestaShop projects. Discover how we've helped businesses succeed online with custom e-commerce solutions.";
const canonicalURL = "https://prestashop.supports.pro/portfolio";
---

<Layout title={pageTitle} description={pageDescription} canonical={canonicalURL}>
  <Header />
  <main>
    <Portfolio />
    <Contact />
  </main>
  <Footer />
</Layout>
